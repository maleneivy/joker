{
  "name": "mio-package",
  "version": "0.2.0",
  "description": "Application that tells jokes",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/maleneivy/joker.git"
  },
  "author": "Malene Ivy <maleneolsen_@hotmail.com>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/maleneivy/joker/issues"
  },
  "homepage": "https://github.com/maleneivy/joker#readme",
  "type": "module",
  "dependencies": {
    "give-me-a-joke": "^0.5.1"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}